{
  "suggest.noselect": false,
  "suggest.enablePreview": true,
  "diagnostic.virtualText": true,
  "coc.preferences.formatOnSaveFiletypes": [
    "javascript",
    "typescript",
    "lua",
    "python",
    "json",
    "markdown",
    "go"
  ],
  "languageserver": {
    "lua": {
      "command": "lua-language-server",
      "filetypes": ["lua"]
    }
  },
  "go.formatTool": "gopls",
  "go.goplsOptions": {
    "formatting.gofumpt": true
  },
  "go.useLanguageServer": true
}
